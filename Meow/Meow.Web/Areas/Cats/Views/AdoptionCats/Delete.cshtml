@model AdoptionCatFormModel
@{
    ViewData["Title"] = "Delete";
}



<div class="body-blur"></div>
<div class="grad"></div>

<br>
@*<h2>Delete</h2>*@
<div class="login">
    <form asp-route-returnUrl="@ViewData["ReturnUrl"]" method="post" enctype="multipart/form-data">
        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-group">
            <input asp-for="Name" class="form-control" placeholder="name" disabled />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input asp-for="Age" class="form-control" placeholder="age" disabled />
            <span asp-validation-for="Age" class="text-danger"></span>
        </div>

        @*<input asp-for="Image" type="file" class="form-control" />*@
        <div class="form-group">
            <input asp-for="Description" class="form-control" placeholder="description" disabled />
            <span asp-validation-for="Description" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input asp-for="Gender" disabled />
            <span asp-validation-for="Gender" class="text-danger"></span>
        </div>

        <button type="submit" class="login-button" style="width:250px">Delete</button>
    </form>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}